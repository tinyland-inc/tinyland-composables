{
  "name": "@tummycrypt/tinyland-composables",
  "version": "0.1.0",
  "description": "Svelte 5 rune-based composables for reactive state management, lifecycle utilities, theming, and Web Worker synchronization",
  "type": "module",
  "license": "Zlib AND LicenseRef-Tinyland-Proprietary",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "svelte": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "svelte-package -i src -o dist",
    "dev": "svelte-package -i src -o dist --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "svelte-check --tsconfig tsconfig.json",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "svelte",
    "svelte5",
    "runes",
    "composables",
    "reactive",
    "lifecycle",
    "theming",
    "web-worker"
  ],
  "author": "Tinyland.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/tinyland-inc/tinyland.dev",
    "directory": "packages/tinyland-composables"
  },
  "peerDependencies": {
    "svelte": ">=5.0.0",
    "runed": ">=0.20.0"
  },
  "peerDependenciesMeta": {
    "runed": {
      "optional": true
    }
  },
  "dependencies": {
    "@tummycrypt/sveltekit-shims": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.0.0",
    "@types/node": "^22.0.0",
    "jsdom": "^25.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "provenance": true
  }
}
